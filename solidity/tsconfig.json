{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES2020",
		"moduleResolution": "node",
		"rootDir": ".",
		"sourceMap": false,
		"inlineSources": false,
		"declaration": false,
		"declarationMap": false,
		"noEmit": false,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"lib": ["ES2022"],
		"typeRoots": ["node_modules/@types", "types"],
		"types": [ "node" ],
	},
	"include": [
		"types/index.d.ts",
		"compile.ts"
	]
}
